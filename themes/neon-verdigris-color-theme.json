  "tokenColors": [
    {
      "name": "Override and reset to defaults",
      "scope": [
        "entity.name.variable",
        "keyword.operator.assignment",
        "meta.template",
        "punctuation.accessor",
        "support.constant",
        "variable"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#a7adba"
      }
    },
    {
      "name": "De-emphasize comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#65737e"
      }
    },
    {
      "name": "Emphasize comment punctuation",
      "scope": "punctuation.definition.comment",
      "settings": {
        "fontStyle": "",
        "foreground": "#00ef97"
      }
    },
    {
      "name": "Emphasize strings",
      "scope": "string",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00ef97"
      }
    },
    {
      "scope": "punctuation.definition.string",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Emphasize constants",
      "scope": ["constant.language", "constant.numeric"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#00ef97"
      }
    },
    {
      "name": "De-emphasize most keywords",
      "scope": ["keyword - keyword.operator", "keyword.control", "storage"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#65737e"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasize operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#00ef97"
      }
    },
    {
      "name": "Emphasize types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c0c5ce"
      }
    },
    {
      "name": "De-emphasize `this` variables",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#65737e"
      }
    },
    {
      "name": "Emphasize template expression punctuation",
      "scope": [
        "punctuation.definition.template-expression",
        "entity.tag.embedded"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#dfe1e8"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasize function / method declaration names",
      "scope": ["meta.definition.function", "meta.definition.method"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00ef97"
      }
    },
    {
      "scope": "storage.type.function.arrow",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00ef97"
      }
    },
    {
      "scope": "meta.function-call",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c0c5ce"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c0c5ce"
      }
    },
    {
      "name": "De-emphasize unimportant punctuation",
      "scope": [
        "keyword.operator.type.annotation",
        "punctuation.definition.parameters",
        "punctuation.definition.seperator",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#65737e"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#6874d5"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#47bacc"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#bacc47"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#cc4756"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00ef97"
      }
    },
    {
      "scope": "entity.name.section.markdown",
      "settings": {
        "foreground": "#c0c5ce"
      }
    },
    {
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dfe1e8"
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c0c5ce"
      }
    },
    {
      "name": "Markdown: Emphasize punctuation",
      "scope": [
        "beginning.punctuation.definition.list.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.raw.markdown"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00ef97"
      }
    },
    {
      "name": "JSON: Override and reset to defaults",
      "scope": "support.type.property-name.json",
      "settings": {
        "fontStyle": "",
        "foreground": "#a7adba"
      }
    },
    {
      "name": "JSON: emphasize non-string values",
      "scope": ["constant.language.json", "constant.numeric.json"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON: emphasize significant punctuation",
      "scope": [
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json"
      ],
      "settings": {
        "foreground": "#00ef97"
      }
    },
    {
      "name": "JSON: de-emphasize common punctuation",
      "scope": [
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json"
      ],
      "settings": {
        "foreground": "#65737e"
      }
    },
    {
      "name": "Groovy: emphasize constant variables",
      "scope": "constant.other.groovy",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c0c5ce"
      }
    },
    {
      "name": "Groovy: emphasize method keys",
      "scope": "constant.other.key.groovy",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c0c5ce"
      }
    },
    {
      "scope": "source.terraform.embedded.source",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#65737e"
      }
    },
    {
      "scope": "entity.other.attribute-name.terraform",
      "settings": {
        "fontStyle": "",
        "foreground": "#c0c5ce"
      }
    },
    {
      "scope": "keyword.other.function.inline.terraform",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#dfe1e8"
      }
    },
    {
      "scope": [
        "keyword.other.section.begin.terraform",
        "keyword.other.section.end.terraform"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00ef97"
      }
    },
    {
